

@model GlicemiaModel
@{
    Layout = "../Shared/_LayoutLogada.cshtml";
}

<head>
    <style>
        body {
            background-color: #f7f7ff;
        }

        #tudo {
            margin: 0px 130px 0px 130px;
        }

        #formulario {
            margin: 20px 30px 0px 250px;
            float: left;
        }

        #formulario2 {
            margin: 20px 250px 0px 100px;
            float: right;
        }

        .btn1 {
            float: right;
        }

        #title {
            font-size: 29px;
        }
    </style>
</head>

<div id="tudo">
    <div id="formulario">
        <div class="form-group">
            <p id="title"><b> Pressão Arterial </b></p>
            <label for="exampleInputEmail1"> Sístole: </label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite a sístole">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Diástole: </label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite a diástole">
        </div>
        <button type="button" class="btn btn-primary btn1">Enviar</button>
    </div>

    <div id="formulario2">
        <div class="form-group">
            <p id="title"><b> Nível Glicêmico </b></p>
            <label for="exampleInputEmail1"> Nível Glicêmico: </label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite o peso">
        </div>
        <button type="button" class="btn btn-primary btn1">Enviar</button>
    </div>
</div>
	
<script>
    function Excluir(Id) {
        window.location.href = "../Glicemia/ExcluirGlicemia" + Id;
    }
</script>
