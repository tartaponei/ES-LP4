

@model GlicemiaModel
@{
    Layout = "../Shared/_LayoutCadastro.cshtml";
}

<div class="container menu">
    <h3> Meu grafico semanal</h3>
    <table class="table table-hover">
        <thead>
            <tr>
                <th> Id</th>
                <th> Nivel glicemico</th>
                <th>  Data de inserção</th>

            </tr>
        </thead>


    </table>
    </div>
    <form asp-controller="Glicemia" asp-action="Glicemia" class="form-horizontal" style="margin-left:15%">
        <div class="form-group">
            <label class="control-label col-sm-2" for="cpf">Nivel glicemico:</label>
            <div class="col-sm-6">
                <input type="number" class="form-control" asp-for="Nivel_glicemico" id="nome" placeholder="Informe sua Pressao" required>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-2" for="cpf">Data:</label>
            <div class="col-sm-6">
                <input type="datetime" class="form-control" asp-for="Data_cadastro" id="nome">
            </div>
        </div>


        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-3">
                <button type="submit" class="btn btn-block btn-info"> Ok</button>
            </div>
        </div>


    </form>
    <script>
        function Excluir(Cpf) {
            window.location.href = "../Glicemia/ExcluirGlicemia" + Id;
        }
    </script>
